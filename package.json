{
  "name": "steven-portfolio",
  "version": "1.0.0",
  "description": "Portfolio website of Steven Coaila Zaa - Full Stack Developer built with Vue 3, Vite, GSAP, and Three.js",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext js,vue --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,vue --report-unused-disable-directives --fix",
    "test": "vitest",
    "test:run": "vitest run --passWithNoTests"
  },
  "dependencies": {
    "gsap": "^3.13.0",
    "three": "^0.179.1",
    "typed.js": "^2.1.0",
    "lucide-vue-next": "0.562.0",
    "vue": "3.5.26"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "10.4.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.7.4",
    "rollup-plugin-visualizer": "^6.0.3",
    "terser": "^5.43.1",
    "vite": "^7.1.3",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "portfolio",
    "vue",
    "gsap",
    "frontend",
    "fullstack"
  ],
  "author": "Steven Coaila Zaa",
  "license": "MIT"
